{
    "name": "@sumit-paul/universal-method",
    "version": "1.0.3",
    "description": "A package that converts natural language queries into typed responses using AI",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc && rollup -c",
        "prepublishOnly": "npm run build",
        "test": "jest",
        "typecheck": "tsc --noEmit"
    },
    "keywords": [
        "ai",
        "openai",
        "gemini",
        "natural language",
        "query",
        "nlp"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@google/generative-ai": "^0.1.3",
        "openai": "^4.0.0"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^24.0.0",
        "@rollup/plugin-node-resolve": "^15.0.0",
        "@rollup/plugin-typescript": "^11.1.6",
        "@types/jest": "^29.5.14",
        "@types/node": "^20.11.24",
        "dotenv": "^16.3.1",
        "jest": "^29.7.0",
        "rollup": "^3.20.0",
        "ts-jest": "^29.2.6",
        "tslib": "^2.8.1",
        "typescript": "^5.3.3"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
